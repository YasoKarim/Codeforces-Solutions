    #include <iostream>
    using namespace std;
    int main(int argc, char** argv) {
    	ios_base::sync_with_stdio(0); cin.tie(NULL); cout.tie(NULL);
    	int t,a,b,c;
    	cin>> t;
    	while(t--){
    		cin>> a >> b >> c;
    		if((a + b == c) || ( a + c == b) || ( b + c == a) || ( b + a == c))
    		cout<<"YES"<<"\n";
    		else
    		cout<<"NO"<<"\n";
    	}
    	return 0;
    }
