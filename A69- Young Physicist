#include <iostream>
using namespace std;
int main(int argc, char** argv) {
	int n,x,y,z,sumx=0,sumy=0,sumz=0;
	cin>>n;
	while(n--){
		cin>>x>>y>>z;
		sumx+=x;
		sumy+=y;
		sumz+=z;
	}
	if(sumx==0 && sumy==0 && sumz==0)
	cout<<"YES";
	else
	cout<<"NO";
	return 0;
}
//Learn about equilibrium (Sum of vector coordinates) 
//Keep Going :D !
