    #include <iostream>
    #include <cstring>
    using namespace std;
    int main(int argc, char** argv) {
    string s;
    cin>>s;
    int up=0,low=0;
    for(int i=0;i<s.size();i++){
    	if(isupper(s[i]))// to check if character 
    	up++;// to count the letters in the character
    	else
    	low++;
    	
    }
    //to see which case we will shift to upper or lower
    if(up>low){
    	char ch;
    for(int i =0;i<s.size();i++){
      ch = toupper(s[i]);
      cout<<ch;
    	}}
    else{
    	char ch;
    	for(int i=0;i<s.size();i++){
    		ch =tolower(s[i]);
    		cout<<ch;
    		
    	}
    	
    	
    }
    	return 0;
    }
